{
  "env": {
    "browser": true,
    "node": true,
  },
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": ["error", {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "never",
        "exports": "never",
        "functions": "ignore",
    }],
  },
}
